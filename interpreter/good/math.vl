function assert(bool cond) {
	if (cond) {
		print("OK!\n");
	} else {
		print("ASSERTION FAILED :(\n");
	}
};

// Test math operations
int a, b, c, d;

a = 10;
b = 20;
c = 300;
d = 500;

assert(a == a);
assert(b == b);
assert(d == d);
assert(a < b);
assert(d > c);
assert(b <= c);
assert(12 * 12 == 144);
assert(a * b == 200);
assert(1000 < 12 * 10 * 10);
a++;
assert(a == 11);
a *= 3;
assert(a == 33);
